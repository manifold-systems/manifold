{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "top level oneOf enum array",
  "type": "array",
  "definitions": {
    "MyEnum": {
      "enum": ["a", "b", "c"]
    }
  },
  "items": {
    "oneOf": [
      {
        "$ref": "#/definitions/MyEnum"
      },
      {
        "enum": ["d", "e"]
      }
    ]
  }
}